body {
	/*Gradient background*/
	background-color: #3f1bde;
	background-image: linear-gradient(160deg, #3f1bde 0%, #1b73de 100%);
}
/*Navbar Styles*/
nav a {
	/*Dictates link color within the navbar*/
	color: #6fddd2;
	text-decoration: none;
}
nav {
	/*Navbar*/
	height: 35px;
	margin-top: -8px;
	margin-right: -8px;
	margin-left: -8px;
	background-color: #413A5E;
	color: white;
}
nav ol{
	/*List of navbar items*/
	padding: 0px;
	margin: 0px 0px 0px 8px;
}
nav li {
	/*Actual navbar items*/
	height: 20px;
	margin-top: 5px;
	margin-bottom: 5px;
	margin-right: 5px;
	display: inline-block;
	position: relative;
}
nav li#navbarPagesDropdown {
	height: 40px;
}
nav li ul {
	/*Dropdown menu initial setup*/
	display: none;
	position: absolute;
	width: 100px;
	top: 20px;
	left: -11px;
}
nav li:hover > ul {
	/*Displays dropdown menu*/
	margin-top: 5px;
	display: block;
}
nav ul{
	/*Settings for dropdown menu*/
	list-style-type: none;
	margin: 10px;
	padding: 0px;
	background-color: #614271;
	border: 1px solid #614271;
}
nav li ul li {
	/*Settings for dropdown menu items*/
	display: block;
	margin: 5px;
	height: 17.5px;
	left:2.5px;
}
.navbarItem {
	font-size: 20px;
}
/*Page Header Styles*/
